{
    "name": "techjobhunt-frontend",
    "version": "0.0.0",
    "description": "Front-end for Tech Job Hunt",
    "private": true,
    "scripts": {
        "test": "jest",
        "start": "next start",
        "dev": "next --hostname local.app.techjobhunt.com",
        "apollo:download": "apollo client:download-schema schema.graphql && yarn --cwd .. install && yarn --cwd .. prettier --config .prettierrc.js --write web-app/schema.graphql && yarn apollo:codegen",
        "apollo:codegen": "rm -rf graphql/generated/ && yarn run apollo client:codegen --globalTypesFile=graphql/generated/graphql-global-types.ts --target=typescript --outputFlat graphql/generated/",
        "apollo:watch": "yarn apollo:codegen --watch",
        "build": "next build",
        "deploy:dev": "dotenv -e now.env.dev -- now --prod",
        "deploy:prod": "dotenv -e now.env.prod -- now --prod"
    },
    "author": "Robert Cooper",
    "license": "MIT",
    "jest": {
        "testPathIgnorePatterns": [
            "<rootDir>/.next/",
            "<rootDir>/node_modules/"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!(react-quill)/)"
        ],
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest",
            "^.+\\.svg$": "jest-svg-transformer",
            ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
        }
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@apollo/react-ssr": "^3.1.3",
        "@emotion/core": "^10.0.28",
        "@emotion/styled": "^10.0.23",
        "@robertcooper/chakra-ui-core": "^1.0.0",
        "@sentry/browser": "^5.15.4",
        "@svgr/webpack": "^5.3.0",
        "@zeit/next-css": "^1.0.1",
        "add": "^2.0.6",
        "apollo": "^2.26.0",
        "apollo-boost": "^0.4.7",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link": "^1.2.13",
        "apollo-link-error": "^1.1.12",
        "apollo-link-http": "^1.5.16",
        "apollo-upload-client": "^12.1.0",
        "chart.js": "^2.9.3",
        "chartjs-adapter-date-fns": "^1.0.0-alpha3",
        "cors": "^2.8.5",
        "date-fns": "^2.11.1",
        "dotenv-flow": "^3.1.0",
        "emotion-normalize": "^10.1.0",
        "emotion-theming": "^10.0.27",
        "express": "^4.17.1",
        "file-loader": "^6.0.0",
        "formik": "2.1.3",
        "graphql": "^14.5.8",
        "graphql-tag": "^2.10.3",
        "immer": "^5.3.2",
        "isomorphic-unfetch": "^3.0.0",
        "lodash.debounce": "^4.0.8",
        "next": "^9.3.4",
        "next-with-apollo": "https://github.com/robertcoopercode/next-with-apollo.git#react-hooks",
        "normalize.css": "^8.0.1",
        "passport": "^0.4.1",
        "passport-github2": "^0.1.12",
        "polished": "^3.5.1",
        "quill": "^1.3.7",
        "react": "^16.13.1",
        "react-chartjs-2": "^2.9.0",
        "react-dom": "^16.13.1",
        "react-dropzone": "^10.2.2",
        "react-icons": "^3.9.0",
        "react-no-ssr": "^1.1.0",
        "react-quill": "^1.3.5",
        "react-stripe-elements": "^6.1.1",
        "react-tippy": "^1.3.4",
        "styled-normalize": "^8.0.7",
        "url-loader": "^3.0.0",
        "uuid": "^3.4.0",
        "yup": "^0.28.3"
    },
    "devDependencies": {
        "@apollo/react-testing": "^3.1.3",
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.9.0",
        "@testing-library/jest-dom": "^5.3.0",
        "@testing-library/react": "^9.4.0",
        "@types/apollo-upload-client": "^8.1.3",
        "@types/chart.js": "^2.9.18",
        "@types/googlemaps": "^3.39.3",
        "@types/jest": "^25.1.5",
        "@types/lodash.debounce": "^4.0.6",
        "@types/node": "^13.11.0",
        "@types/react": "^16.9.32",
        "@types/react-dom": "^16.9.6",
        "@types/react-no-ssr": "^1.1.1",
        "@types/react-stripe-elements": "^6.0.4",
        "@types/uuid": "^7.0.2",
        "@types/yup": "^0.26.34",
        "babel-jest": "^25.2.6",
        "babel-plugin-emotion": "^10.0.29",
        "dotenv-cli": "^3.1.0",
        "jest": "^25.2.6",
        "jest-svg-transformer": "^1.0.0",
        "jest-transform-css": "^2.0.0",
        "now": "^18.0.0",
        "typescript": "^3.8.3"
    },
    "resolutions": {
        "graphql": "14.5.8"
    },
    "engines": {
        "node": "12",
        "yarn": "^1.16.0"
    }
}
