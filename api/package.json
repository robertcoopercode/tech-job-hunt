{
    "name": "techjobhunt-backend",
    "description": "Back-end for Tech Job Hunt",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "ts-node src/index.ts",
        "dev": "ts-node-dev --no-notify --respawn --transpileOnly ./src/",
        "deploy": "now --prod",
        "prisma:deploy": "prisma deploy",
        "prisma:deploy:dev": "prisma deploy --env-file .env.dev",
        "prisma:deploy:prod": "prisma deploy --env-file .env.prod",
        "prisma:token": "prisma token",
        "prisma:token:dev": "prisma token --env-file .env.dev",
        "prisma:token:prod": "prisma token --env-file .env.prod",
        "prisma:post-deploy": "nexus-prisma-generate --client ./src/generated/prisma-client --output ./src/generated/nexus-prisma && rm ./src/generated/nexus-prisma/nexus-prisma.ts",
        "seed": "prisma seed",
        "stripe": "stripe listen --forward-to local.api.techjobhunt.com:4000/stripe"
    },
    "dependencies": {
        "amplitude": "^4.0.1",
        "aws-sdk": "^2.649.0",
        "bcrypt": "^3.0.7",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.4",
        "cuid": "^2.1.8",
        "date-fns": "^2.9.0",
        "dotenv": "^8.2.0",
        "dotenv-flow": "^3.1.0",
        "express-session": "^1.17.0",
        "formidable": "^1.2.1",
        "graphql": "^14.5.8",
        "graphql-upload": "^8.1.0",
        "graphql-yoga": "^1.18.3",
        "jsonwebtoken": "^8.5.1",
        "nexus": "0.12.0-beta.14",
        "nexus-prisma": "^0.3.8",
        "node-fetch": "^2.6.0",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^2.0.0",
        "prisma-client-lib": "^1.34.8",
        "stripe": "^7.15.0",
        "ts-node": "^8.5.2",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/faker": "^4.1.9",
        "@types/formidable": "^1.0.31",
        "@types/graphql-upload": "^8.0.3",
        "@types/jsonwebtoken": "^8.3.5",
        "@types/node": "^12.12.8",
        "@types/passport": "^1.0.1",
        "@types/passport-google-oauth20": "^2.0.3",
        "@types/stripe": "^7.13.19",
        "@types/uuid": "^3.4.6",
        "faker": "^4.1.0",
        "knex": "^0.20.10",
        "mysql": "^2.18.1",
        "nexus-prisma-generate": "^0.3.7",
        "now": "^16.7.0",
        "prisma": "^1.34.10",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "3.8.3"
    },
    "engines": {
        "node": "12",
        "yarn": "^1.16.0"
    },
    "resolutions": {
        "graphql": "^14.5.8"
    }
}
